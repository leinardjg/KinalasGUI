<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxWidth="300.0" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kinalas.gui.setup.KinalasSetup">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <VBox alignment="CENTER_LEFT" maxWidth="300.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="No employee found in database. Create new admin:">
               <VBox.margin>
                  <Insets bottom="16.0" />
               </VBox.margin>
            </Label>
            <VBox HBox.hgrow="ALWAYS">
               <Label layoutX="14.0" layoutY="168.0" text="First name" />
               <TextField fx:id="firstNameField" HBox.hgrow="ALWAYS" />
               <padding>
                  <Insets right="1.0" />
               </padding>
            </VBox>
            <VBox HBox.hgrow="ALWAYS">
               <Label layoutX="14.0" layoutY="168.0" text="Last name" />
               <TextField fx:id="lastNameField" HBox.hgrow="ALWAYS" />
               <padding>
                  <Insets left="1.0" />
               </padding>
            </VBox>
            <VBox alignment="CENTER">
               <Button fx:id="submitButton" text="Submit" />
                  <VBox.margin>
                     <Insets top="8.0" />
                  </VBox.margin>
            </VBox>
         </children>
      </VBox>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</AnchorPane>
